data.centers.sort(function(a, b) {
  return a.DisplayOrder - b.DisplayOrder;
});

// Extract and display center names in the sorted order
const centerNames = [];
for (let i = 0; i < data.centers.length; i++) {
  centerNames.push(data.centers[i].center_name);
}

console.log("Center Names in Display Order:");
console.log(centerNames);
